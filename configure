### Check for virtual base class address-offset bug

{ $as_echo "$as_me:${as_lineno-$LINENO}: checking whether virtual base class address-offsets are ok" >&5
$as_echo_n "checking whether virtual base class address-offsets are ok... " >&6; }

if test "$cross_compiling" = yes; then :
  { { $as_echo "$as_me:${as_lineno-$LINENO}: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
as_fn_error $? "cannot run test program while cross compiling
See \`config.log' for more details" "$LINENO" 5; }
else
  cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
void* p1 = 0; void* p2 = 0;

struct A {
  virtual ~A() {};
  virtual void foo() { delete this; }
  int i;
};

struct B : public virtual A {
  virtual ~B() { p2 = (void*)this; }
};

int main()
{
  B* b = new B;
  p1 = (void*)b;
  b->foo();

  return (p1 == p2) ? 0 : 1;
}
_ACEOF
if ac_fn_cxx_try_run "$LINENO"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; };
 as_fn_error $? "$CXX has a virtual base class address-offset bug" "$LINENO" 5
fi
rm -f core *.core core.conftest.* gmon.out bb.out conftest$ac_exeext \
  conftest.$ac_objext conftest.beam conftest.$ac_ext
fi


